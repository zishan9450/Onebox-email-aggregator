{
  "name": "onebox-email-aggregator",
  "version": "1.0.0",
  "description": "Feature-rich email aggregator with AI categorization and real-time sync",
  "main": "dist/server.js",
  "scripts": {
    "setup": "node setup.js",
    "demo": "node demo.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "docker:elasticsearch": "docker-compose up -d elasticsearch",
    "docker:down": "docker-compose down",
    "docker:full": "docker-compose up -d",
    "logs": "docker-compose logs -f"
  },
  "keywords": [
    "email",
    "imap",
    "ai",
    "elasticsearch",
    "real-time"
  ],
  "author": "OneBox Team",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@slack/web-api": "^6.10.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "elasticsearch": "^16.7.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "imap": "^0.8.19",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.6.5",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^2.4.1",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
